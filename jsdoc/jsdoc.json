{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "includePattern": ".jsx?$",
    "include": ["src/js", "package.json", "README.md"],
    "excludePattern": "(node_modules/|docs|__mocks__|__tests__)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "verbose": true,
    "template": "jsdoc/template"
  },
  "templates": {
    "referenceTitle": "translationCore",
    "search": {
      "apiKey": "algolia api key",
      "indexName": "translationCore",
      "hitsPerPage": 7
    }
  }
}
