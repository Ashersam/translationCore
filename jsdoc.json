{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".jsx?$",
    "include": ["src/js", "package.json", "README.md"],
    "excludePattern": "(node_modules/|docs|__mocks__|__tests__)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "verbose": true,
    "template": "node_modules/docdash"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false
  }
}
